{
  "name": "docusaurus-guides",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint -c ./.eslintrc.json '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}' --report-unused-disable-directives --max-warnings 0",
    "lint-biome": "biome lint ./src",
    "lint-fix": "eslint --fix -c .eslintrc.cjs '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}'",
    "format": "biome format --verbose ./src",
    "format-prettier": "prettier . --check --config .prettierrc.mjs",
    "format-fix-prettier": "prettier . --write --config .prettierrc.mjs",
    "test": "node --test",
    "build": "docusaurus build",
    "dev": "docusaurus start",
    "dev-heb": "docusaurus start --locale he",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations-heb": "docusaurus write-translations --locale he",
    "translate-docs": "./toolbox/translateDocs.sh",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.1",
    "@docusaurus/plugin-content-docs": "^3.5.1",
    "@docusaurus/preset-classic": "3.5.1",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@docusaurus/module-type-aliases": "3.5.1",
    "@docusaurus/tsconfig": "^3.5.1",
    "@docusaurus/types": "3.5.1",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ezlint": "^1.6.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
