{
  "name": "docusaurus-guides",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint-biome": "biome lint ./src",
    "lint-fix": "eslint --fix",
    "format": "biome format --verbose ./src",
    "format-prettier": "prettier . --check --config .prettierrc.mjs",
    "format-fix-prettier": "prettier . --write --config .prettierrc.mjs",
    "test": "node --test",
    "build": "docusaurus build",
    "dev": "docusaurus start",
    "dev-heb": "docusaurus start --locale he",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations-heb": "docusaurus write-translations --locale he",
    "translate-docs": "./toolbox/translateDocs.sh",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.0",
    "@docusaurus/plugin-content-docs": "^3.9.0",
    "@docusaurus/preset-classic": "3.9.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@docusaurus/module-type-aliases": "3.9.0",
    "@docusaurus/tsconfig": "^3.9.0",
    "@docusaurus/types": "3.9.0",
    "@eslint/js": "^9.32.0",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^19.1.9",
    "eslint": "^9.32.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
