{
  "name": "docusaurus-guides",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint-biome": "biome lint ./src",
    "lint-fix": "eslint --fix",
    "format": "biome format --verbose ./src",
    "format-prettier": "prettier . --check --config .prettierrc.mjs",
    "format-fix-prettier": "prettier . --write --config .prettierrc.mjs",
    "test": "node --test",
    "build": "docusaurus build",
    "dev": "docusaurus start",
    "dev-heb": "docusaurus start --locale he",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations-heb": "docusaurus write-translations --locale he",
    "translate-docs": "./toolbox/translateDocs.sh",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.3",
    "@docusaurus/plugin-content-docs": "^3.6.3",
    "@docusaurus/preset-classic": "3.6.3",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@docusaurus/module-type-aliases": "3.6.3",
    "@docusaurus/tsconfig": "^3.6.3",
    "@docusaurus/types": "3.6.3",
    "@eslint/js": "^9.17.0",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.3.3",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.5.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-55955c9-20241229",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.19.0"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": ["last 3 chrome version", "last 3 firefox version", "last 5 safari version"]
  },
  "engines": {
    "node": ">=18.0"
  }
}
